{
    "priority": 1,
    "request": {
        "method": "GET",
        "url": "/passport",
        "bodyPatterns": [
            {
                "equalToJson": "{ \"passport_id\": \"P002\" }",
                "ignoreArrayOrder": true,
                "ignoreExtraElement": true
            }
        ]
    },
    "response": {
        "status": 200,
        "headers": {
            "Content-Type": "application/json"
        },
        "jsonBody": {
            "passport_id": "P002",
            "first_name": "Bob",
            "last_name": "Smith"
        }
    }
}
